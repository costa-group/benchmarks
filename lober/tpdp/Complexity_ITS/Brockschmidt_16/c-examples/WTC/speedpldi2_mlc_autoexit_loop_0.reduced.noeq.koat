(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS init_node))
(VAR A B C)
(RULES
 init_node(A,B,C) -> Com_1(evalspeedpldi2bb5in(A,B,C))
 evalspeedpldi2bb5in(A,B,C) -> Com_1(evalspeedpldi2bb5in(A,B+1,C-1)) :|: 1*A-1*B >= 1 && 1*C >= 1
 evalspeedpldi2bb5in(A,B,C) -> Com_1(evalspeedpldi2bb5in(A,0,C)) :|: 1*C >= 1 && -1*A+1*B >= 0
 evalspeedpldi2bb5in(A,B,C) -> Com_1(exit_node(A,B,C)) :|: -1*C >= 0
)
