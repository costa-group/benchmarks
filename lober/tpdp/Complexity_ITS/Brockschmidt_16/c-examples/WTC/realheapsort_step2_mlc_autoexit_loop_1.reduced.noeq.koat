(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS init_node))
(VAR A B C D u1 u2)
(RULES
 init_node(A,B,C,D) -> Com_1(evalrealheapsortstep2bb11in(A,B,C,D))
 evalrealheapsortstep2bb11in(A,B,C,D) -> Com_1(evalrealheapsortstep2bb11in(A,B+1,u1,u2)) :|: 1*A-1*B >= 2 && 2*u1-1*A+1*B >= -2
 evalrealheapsortstep2bb11in(A,B,C,D) -> Com_1(exit_node(A,B,C,D)) :|: -1*A+1*B >= -1
)
