(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS init_node))
(VAR A B C u1)
(RULES
 init_node(A,B,C) -> Com_1(evalrealheapsortstep1bb6in(A,B,C))
 evalrealheapsortstep1bb6in(A,B,C) -> Com_1(evalrealheapsortstep1bb6in(A,B+1,B-u1)) :|: 1*A-1*B >= 1 && 1*u1 >= 0
 evalrealheapsortstep1bb6in(A,B,C) -> Com_1(exit_node(A,B,C)) :|: -1*A+1*B >= 0
)
