(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS init_node))
(VAR A B C)
(RULES
 init_node(A,B,C) -> Com_1(eval(A,B,C))
 eval(A,B,C) -> Com_1(eval(A-1,B,C-1)) :|: 1*A-1*B >= 1 && -1*B+1*C >= 1
 eval(A,B,C) -> Com_1(exit_node(A,B,C)) :|: -1*A+1*B >= 0
 eval(A,B,C) -> Com_1(exit_node(A,B,C)) :|: 1*B-1*C >= 0
)
