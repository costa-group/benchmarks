(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS f0))
(VAR x n)
(RULES

f0(n,x) -> f1(n,x)

f1(n,x) -> f1(n-1,0) :|: n>0 && x>0
f1(n,x) -> f1(n-1,1) :|: n>0 && x<=0

f1(n,x) -> f2(n,x) :|: n<=0
)

