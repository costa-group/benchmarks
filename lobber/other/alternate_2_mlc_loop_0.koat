(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS init_node))
(VAR A B C D)
(RULES
 init_node(A,B,C,D) -> Com_1(l1(A,B,C,D))
 l1(A,B,C,D) -> Com_1(l1(A+D,B,C,D-1)) :|: 1*D >= 1
 l1(A,B,C,D) -> Com_1(l1(A+D,B,C,D-1)) :|: 1*D >= 1
 l1(A,B,C,D) -> Com_1(exit_node(A,B,C,D)) :|: -1*D >= 1
 l1(A,B,C,D) -> Com_1(exit_node(A,B,C,D)) :|: -1*D >= 0
)
