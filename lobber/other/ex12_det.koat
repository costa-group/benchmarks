# Running example with a previous loop that increases quadratically the value of 'x'

(GOAL COMPLEXITY)
(STARTTERM (FUNCTIONSYMBOLS l0))
(VAR x y z)
(RULES

l0(x,y,z) -> l00(x,y,z)

# Quadratic increase
l00(x,y,z) -> l00(x+z,y,z-1) :|: z > 0
l00(x,y,z) -> l1(x,y,z)      :|: z <= 0

#e1
l1(x,y,z) -> l2(x,y,z)  :|: x < 0  
#e2
l1(x,y,z) -> l2(x,y,z)  :|: y <= 0

#t1
l1(x,y,z) -> l1(x-1,y,z) :|: x >= 0 && y > 0
#t2
l1(x,y,z) -> l1(x,y-1,z) :|: x >= 0 && y > 0

)
